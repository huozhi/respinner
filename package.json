{
  "name": "respinner",
  "version": "3.0.7",
  "description": "React SVG spinner components",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "module": "./dist/index.esm.js"
  },
  "types": "types.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prepublishOnly": "npm run clean && npm run build",
    "build:docs": "yarn build && vite build ./docs",
    "build": "bunchee src/index.js",
    "dev": "yarn build --watch",
    "dev:docs": "vite dev ./docs --port 3000"
  },
  "sideEffects": false,
  "files": [
    "*.md",
    "dist",
    "types.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/respinner.git"
  },
  "keywords": [
    "react",
    "loading"
  ],
  "author": "huozhi",
  "license": "MIT",
  "homepage": "https://github.com/huozhi/respinner#readme",
  "devDependencies": {
    "bunchee": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "respinner": "link:./",
    "vite": "^3.1.3"
  },
  "peerDependencies": {
    "react": "*"
  }
}
